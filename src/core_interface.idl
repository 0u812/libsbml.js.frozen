[Prefix="libsbml::"]
interface Reaction {
  DOMString getId();
};

[Prefix="libsbml::"]
interface Model {
  long getNumReactions();
  Reaction getReaction(long i);
};

[Prefix="libsbml::"]
interface SBMLDocument {
  long getNumErrors();
  Model getModel();
};

[Prefix="libsbml::"]
interface SBMLReader {
  void SBMLReader();
  SBMLDocument readSBMLFromString(DOMString sbmlstr);
};
